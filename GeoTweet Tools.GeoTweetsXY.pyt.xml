<?xml version="1.0"?>
<metadata xml:lang="en"><Esri><CreaDate>20130104</CreaDate><CreaTime>14552900</CreaTime><ArcGISFormat>1.0</ArcGISFormat><SyncOnce>TRUE</SyncOnce><ModDate>20140312</ModDate><ModTime>155541</ModTime><scaleRange><minScale>150000000</minScale><maxScale>5000</maxScale></scaleRange><ArcGISProfile>ItemDescription</ArcGISProfile></Esri><dataIdInfo><idCitation><resTitle>GeoTweets by XY</resTitle></idCitation><idAbs>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P STYLE="margin:0 0 14 0;"&gt;&lt;SPAN&gt;GeoTweets by XY searches a radius of the input location and returns a table of all geo-enabled tweets within that area.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN /&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</idAbs><searchKeys><keyword>social media</keyword><keyword>twitter</keyword><keyword>tweets</keyword><keyword>search</keyword></searchKeys><idCredit>Timothy Hales - Esri</idCredit><resConst><Consts><useLimit>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The &lt;/SPAN&gt;&lt;A href="https://dev.twitter.com/terms/api-terms"&gt;&lt;SPAN&gt;Twitter API Term of Service &lt;/SPAN&gt;&lt;/A&gt;&lt;SPAN&gt;restrict the storage of geographic location information contained in Twitter Content; therefore, a tweet information table is all that can currently be returned.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</useLimit></Consts></resConst></dataIdInfo><distInfo><distributor><distorFormat><formatName>ArcToolbox Tool</formatName></distorFormat></distributor></distInfo><tool name="GeoTweetsXY" displayname="GeoTweets by XY" toolboxalias="GeoTweets" xmlns=""><parameters><param name="Xcoordinate" displayname="Search X Coordinate" type="Required" direction="Input" datatype="Double" expression="Xcoordinate"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;X coordinate in decimal degrees (Longitude).&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="Ycoordinate" displayname="Search Y Coordinate" type="Required" direction="Input" datatype="Double" expression="Ycoordinate"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Y coordinate in decimal degrees (Latitude).&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="radius" displayname="Search Radius" type="Required" direction="Input" datatype="Long" expression="radius"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P STYLE="margin:0 0 14 0;"&gt;&lt;SPAN /&gt;&lt;/P&gt;&lt;P STYLE="margin:0 0 14 0;"&gt;&lt;SPAN&gt;Search radius for each location (integer value).&lt;/SPAN&gt;&lt;/P&gt;&lt;P STYLE="margin:0 0 14 0;"&gt;&lt;SPAN&gt;It is recommended to use a small radius (1 mile) to return the most tweets.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="units" displayname="Search Units" type="Required" direction="Input" datatype="String" expression="mi | km"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P STYLE="margin:0 0 14 0;"&gt;&lt;SPAN&gt;Search radius units. &lt;/SPAN&gt;&lt;/P&gt;&lt;P STYLE="margin:0 0 14 0;"&gt;&lt;SPAN&gt;This may be miles (mi) or kilometers (km).&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="outputPath" displayname="Output Tweet Table" type="Required" direction="Output" datatype="Table" expression="outputPath"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;P&gt;&lt;SPAN&gt;The output table to be created. &lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN&gt;This table will store the following for each tweet&lt;/SPAN&gt;&lt;SPAN STYLE="font-size:10pt"&gt;: &lt;/SPAN&gt;&lt;SPAN&gt;&amp;lt;User Name&amp;gt;, &amp;lt;Tweet (text)&amp;gt;, &amp;lt;Tweet Time&amp;gt;, &amp;lt;Tweet ID&amp;gt;.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;</dialogReference></param></parameters><summary>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P STYLE="margin:0 0 14 0;"&gt;&lt;SPAN&gt;GeoTweets by XY searches a radius of the input location and returns a table of all geo-enabled tweets within that area.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN /&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</summary></tool><mdHrLv><ScopeCd value="005"/></mdHrLv></metadata>
